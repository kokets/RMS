                        <h3><a asp-controller="MoUView" asp-action="Index">Back To List</a></h3>
                        <div class="view-mo-menu">
                            <div class="view-mou-menu">
                                <a onclick="animationsHandler.showTable('table2')">View MoU request</a>
                                <a onclick="animationsHandler.showTable('table3')">Comments on MoU request</a>
                            </div>
                        </div>

                        <div id="table3" class="table-template" style="display: none;">
                        </div>

                        <div id="table2" class="table-template" style="display: none;">
                            <table id="event-table">
                                <tr class="data-row">
                                    <td data-label="Budget Year" class="color-pellette-4 short-column">

                                        Reference number
                                    </td>
                                    <td data-label="Mou Unit">
                                        @Model.MouViewRequestById.ReferenceNumber
                                    </td>
                                </tr>
                                <tr>
                                    <td data-label="Budget Year" class="color-pellette-4">Request by:	</td>
                                    <td data-label="Mou Unit">
                                        @Model.MouViewRequestById.Requester
                                    </td>
                                </tr>
                                <tr>
                                    <td data-label="Budget Year" class="color-pellette-4">Type of agreement:		</td>
                                    <td data-label="Mou Unit">
                                    @*  National/Regional/International*@
                                    @Model.MouViewRequestById.AgreementType


                                    </td>
                                </tr>
                                   <tr>
                                    <td data-label="Budget Year" class="color-pellette-4">MoU requested:		</td>
                                    <td data-label="Mou Unit">
                                    @* The Partnership for African Social and Governance Research (PASGR) *@                                       
                                        @Model.MouViewRequestById.MouRequest

                                    </td>
                                </tr>
                                <tr>
                                    <td data-label="Budget Year" class="color-pellette-4">MoU from:		</td>
                                    <td data-label="Mou Unit">
                                        @Model.MouViewRequestById.Division

                                    </td>
                                </tr>

                                <tr>
                                    <td data-label="Budget Year" class="color-pellette-4">completed document:			</td>
                                    <td data-label="Mou Unit">
                                        Completed document
                                    </td>
                                </tr>
                                <tr>
                                    <td data-label="Budget Year" class="color-pellette-4">View vetted document:		</td>
                                    <td data-label="Mou Unit">
                                        Vetted document
                                    </td>
                                </tr>
                             
                                <tr>
                                    <td data-label="Budget Year" class="color-pellette-4">OCEO approved		</td>
                                    <td data-label="Mou Unit">

No                                    </td>
                                </tr>
                                   <tr>
                                    <td data-label="Budget Year" class="color-pellette-4">CEO approved		</td>
                                    <td data-label="Mou Unit">

No                                    </td>
