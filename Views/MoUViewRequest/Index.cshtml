 <div class="section5 color-pellette-5">
        <div class="row collapse" data-equalizer>
            <div class="large-12 columns ">
                    <section class="step-wizard">
                        <ul class="step-wizard-list">
                            <li class="step-wizard-item" onclick="startAnimation(1)">
                                <span class="progress-count">1</span>
                                <span class="progress-label">Request MoU</span>
                            </li>
                            <li class="step-wizard-item current-item" onclick="startAnimation(2)">
                                <span class="progress-count">2</span>
                                <span class="progress-label">Upload MoU</span>
                            </li>
                            <li class="step-wizard-item" onclick="startAnimation(3)">
                                <span class="progress-count">3</span>
                                <span class="progress-label">Legal Vetting the MoU</span>
                            </li>
                            <li class="step-wizard-item" onclick="startAnimation(4)">
                                <span class="progress-count">4</span>
                                <span class="progress-label">Send OCEO</span>
                            </li>
                            <li class="step-wizard-item" onclick="startAnimation(5)">
