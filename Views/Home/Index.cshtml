	<style>



		/* Drop Down Styles */
		nav .dropdowng {
			position: relative;
			display: inline-block;
			list-style: none;
			margin: 0;
			padding: 0;
		}

		nav .dropdowng-menu {
			position: absolute;
			top: 100%;
			left: 0;
			list-style: none;
			font-family: Arial;
			width: 200px;
			padding: 0;
			text-align: center;
			display: none;
		}

		nav .subg-menu {
			position: absolute;
			top: 0;
			left: 100%;
			list-style: none;
			font-family: Arial;
			width: 200px;
			padding: 0;
			text-align: center;
			display: none;
		}

			nav .dropdowng-menu li,
			nav .subg-menu li {
				position: relative;
			}

				nav .dropdowng-menu li a,
				nav .subg-menu li a {
					display: block;
					color: #ecf0f1;
					text-decoration: none;
					padding: 10px;
					position: relative;
				}

				nav .dropdowng-menu li:first-child a::after,
				nav .subg-menu li:first-child a::after {
					content: "\25B6";
					float: right;
					margin-right: 5px;
				}

		/* Show sub-menu on hover */
		nav .dropdowng:hover .dropdowng-menu,
		nav .has-submenu:hover .subg-menu {
			display: block;
		}

		nav .dropdowng-menu li .disabled,
		nav .subg-menu li .disabled {
			color: #888;
			pointer-events: none;
		}

		nav .has-submenu > a .arrow-icon {
			float: right;
			margin-right: 5px;
		}

		/* Show sub-menu on hover */
		nav .dropdowng:hover .dropdowng-menu,
		nav .has-submenu:hover .subg-menu {
			display: block;
		}
		/*dropdown menu start*/

	</style>
</head>
