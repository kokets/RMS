    <style>

        tr {
            transition: all 1s ease-in-out;
        }

            tr.slide-out {
                transform: translateX(100%);
            }

            tr.slide-in {
                transform: translateX(0);
            }

        .short-column {
            width: 30%;
        }

        .view-mo-menu {
            display: flex;
            justify-content: center;
            padding: 10px;
            margin-top: -20px; /* Adjust this value as needed */
        }

        .view-mou-menu {
            display: flex;
            gap: 20px;
            background-color: #f2f2f2;
            padding: 5px;
            border-radius: 5px;
            list-style: none;
            margin: 0;
        }

            .view-mou-menu a {
                position: relative;
                width: 33.333%;
                display: table-cell;
                text-decoration: none;
                font-weight: bold;
                padding: 10px 20px;
                transition: 0.2s ease color;
                border-right: 1px solid #ccc; /* Add right border to create a line separator */
            }

                .view-mou-menu a:last-child {
                    border-right: none; /* Remove right border for the last link */
                }

                .view-mou-menu a:before, .view-mou-menu a:after {
                    position: absolute;
                    border-radius: 50%;
                    transform: scale(0);
                    transition: 0.2s ease transform;
                }

                .view-mou-menu a:after {
                    top: 5%;
                    left: 18px;
                    width: 4px;
                    height: 4px;
                }
    </style>
</head>
