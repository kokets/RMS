    <div class="section3 color-pellette-8 text-center">
        <div class="row padding_40 ">
            <div class="columns small-8 medium-centered small-centered large-centered">
                <h4>Edit License Types</h4>
                <p class="anim-text">Social science that makes a difference</p>
            </div>
            <a asp-controller="Home" asp-action="Index"
               class="button radius padding_10">Back</a>
            <a asp-controller="Home" asp-action="Index"
               class="button radius padding_10">Home</a>
        </div>
    </div>


    <div class="section5 color-pellette-5  ">
        <div class="row collapse  " data-equalizer>
            <div class="large-12 medium-12 small-12 columns">
                <div class="large-12 medium-12 small-12 columns padding_10 data-equalizer-watch">


                    <div class="content-blocks content-blocks-0  ">
                        <h3>   <a asp-controller="LicenseMLTypes" asp-action="Index">Back to License Type listing</a> </h3>

                        <h3>   <a asp-controller="License" asp-action="Index">Back To Dashboard</a> </h3>

                        <form id="giftForm" asp-action="Index" method="post" onsubmit="return MethodHandler.GiftValidateForm()">
                            @Html.AntiForgeryToken()

                            <input type="hidden" name="TypeId" value="@Model.TypeId" />

                            <div class="">
                                <div class="search-section">

                                    <div class="medium-12 cell ">
                                        <label>
                                            Type
                                            <span class="mandatory-star">*</span>
                                            @Html.TextBoxFor(model => model.Type, new { required = "required"  ,id = "GiftSponsor"})
                                            @Html.ValidationMessageFor(model => model.Type)
                                        </label>
                                    </div>
                                    <div class="medium-12 cell">
                                        <label>
                                            Status
                                            <span class="mandatory-star">*</span>
                                            @Html.DropDownListFor(model => model.Status, DropdownHelpers.GetStatusOption(), "--Please Select--", new { required = "required" ,id = "Status"})
                                            @Html.ValidationMessageFor(model => model.Status)
                                        </label>
                                    </div>
                                    <div class="medium-12 cell " style=" display: flex;gap: 10px;">
                                        <button type="submit" style="background-color: grey;height: 35px;  width: 100px; border-radius: 6px;color: white;font-family: inherit; margin-top: 15px;">
                                            Save
                                        </button>
                                        <button asp-controller="GiftDisplay" asp-action="Index" style="background-color: grey;height: 35px;  width: 100px; border-radius: 6px;color: white;font-family: inherit; margin-top: 15px;">Cancel</button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>



