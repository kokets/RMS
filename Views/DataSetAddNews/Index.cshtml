    <script>
        // JavaScript code to handle form toggle
        function toggleForm(formId) {
            const form = document.getElementById(formId);
            const otherForms = document.querySelectorAll('.table-template:not(#' + formId + ')');

            if (form.style.display === 'none') {
                // Show the selected form
                form.style.display = 'block';
                // Hide other forms
                otherForms.forEach((otherForm) => {
                    otherForm.style.display = 'none';
                });
            } else {
                // Hide the selected form
                form.style.display = 'none';
            }
        }

        function toggleForms(showFormId, hideFormId) {
            const showForm = document.getElementById(showFormId);
            const hideForm = document.getElementById(hideFormId);

            if (hideForm) {
                hideForm.style.display = 'none';
            }

            if (showForm) {
                showForm.style.display = 'block';
            }
        }








    </script>
    <script>
        document.getElementById('form-selector').addEventListener('change', function () {
            const selectedFormId = this.value;

            // Hide the current form (if any)
            const currentForm = document.querySelector('div[id^="form-"]:not([style*="display: none;"])');
            if (currentForm) {
                currentForm.style.display = 'none';
            }

            // Show the selected form
            if (selectedFormId) {
                const selectedForm = document.getElementById(selectedFormId);
                if (selectedForm) {
                    selectedForm.style.display = 'block';
                }
            }
        });



        function handleSelectChange() {
            const selectedFormId = document.getElementById('form-1').value;

            const forms = document.getElementById('form-container').children;
            for (let i = 0; i < forms.length; i++) {
                forms[i].style.display = forms[i].id === selectedFormId ? 'block' : 'none';
            }
        }

        function goToForm(formId) {
            const forms = document.getElementById('form-1').children;
            for (let i = 0; i < forms.length; i++) {
                forms[i].style.display = forms[i].id === formId ? 'block' : 'none';
            }
            document.getElementById('form-selector').value = formId;
        }

    </script>







    <script>
        function handleSelectChange() {
            const formSelector = document.getElementById("form-selector");
            const selectedFormId = formSelector.value;

            // Hide all forms and modals
            const forms = document.querySelectorAll("[id^='form-']");
            forms.forEach((form) => {
                form.style.display = "none";
            });

            // Show the selected form or modal
            if (selectedFormId) {
                if (selectedFormId === "delete-popup") {
                    openModal(selectedFormId);
                } else {
                    const selectedForm = document.getElementById(selectedFormId);
                    if (selectedForm) {
                        selectedForm.style.display = "block";
                    }
                }
            }
        }

        function openModal(modalId) {
            const modal = document.getElementById(modalId);
            if (modal) {
                modal.style.display = "block";
            }
        }

        function closeModal(modalId) {
            const modal = document.getElementById(modalId);
            if (modal) {
                modal.style.display = "none";
            }
        }

    </script>

